pcb-component basic:
  pin-properties:
    [pin:Ref | pads:Ref ...]
      [ VDD | p[8]]
      [ DIN  | p[1]]
      [ EN | p[2]]
      [ CLK | p[3]]
      [ DOUT | p[5]]
      [ ALERT | p[6]]
      [ NC    | p[7]]
      [ GND | p[4]]

  val box = BoxSymbol(self)
  val symb = create-symbol(box)
  assign-symbol(symb)

  val lp = create-landpattern $ SOIC-N(
    num-leads = 8,
    lead-span = min-max(5.8, 6.2),
    package-length = 4.5 +/- 0.10,
    density-level = DensityLevelC
  )
  assign-landpattern(lp)

pcb-component basic-grid:
  pin-properties:
    [pin:Ref | pads:Ref ... | side:Dir | row:Int]
      [ VDD | p[8] | Right | 3]
      [ VDD2 | p[9] | Right | 3]
      [ DIN  | p[1] | Left | 1]
      [ EN | p[2] | Left | 1]
      [ CLK | p[3] | Left | 1]
      [ DOUT | p[5] | Right | 1]
      [ ALERT | p[6] | Right | 2]
      [ NC    | p[7] | Left | 3]
      [ GND | p[4] | Right | 0]
      [ GND2 | p[10] | Right | 0]

  val box = BoxSymbol(self)
  set-grid(box, [4,2])
  set-row(3, find-pins-by-regex(box, "GND.*"))
  set-row(0, self.VDD, self.VDD2)
  set-row(3, [self.CLK])

  assign-symbol(create-symbol(box))
  val lp = create-landpattern $ SOIC-N(
    num-leads = 10,
    lead-span = min-max(5.8, 6.2),
    package-length = 4.5 +/- 0.10,
    density-level = DensityLevelC
  )
  assign-landpattern(lp)

